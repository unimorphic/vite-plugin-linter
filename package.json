{
  "name": "vite-plugin-linter",
  "description": "Plugin for linting files with Vite",
  "license": "MIT",
  "version": "2.1.1",
  "scripts": {
    "build": "unbuild",
    "prepare": "npm run build"
  },
  "bin": "./dist/lintCommand.mjs",
  "dependencies": {
    "@rollup/pluginutils": "5.1.0",
    "@types/eslint": "8.56.2",
    "chokidar": "3.5.3",
    "eslint": "8.56.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@types/node": "20.11.5",
    "prettier": "3.2.4",
    "rollup": "4.9.6",
    "unbuild": "2.0.0",
    "vite": "5.0.12"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://bitbucket.org/unimorphic/vite-plugin-linter",
  "keywords": [
    "eslint",
    "typescript",
    "vite-plugin"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://unimorphic@bitbucket.org/unimorphic/vite-plugin-linter.git"
  },
  "types": "dist/index.d.ts"
