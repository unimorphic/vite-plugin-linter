{
  "name": "vite-plugin-linter",
  "description": "Plugin for linting files with Vite",
  "license": "MIT",
  "version": "3.0.1",
  "scripts": {
    "build": "unbuild",
    "prepare": "npm run build"
  },
  "bin": "./dist/lintCommand.mjs",
  "dependencies": {
    "@rollup/pluginutils": "5.1.3",
    "@types/eslint": "9.6.1",
    "chokidar": "4.0.1",
    "eslint": "9.14.0",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "prettier": "3.3.3",
    "rollup": "4.25.0",
    "unbuild": "2.0.0",
    "vite": "5.4.10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://bitbucket.org/unimorphic/vite-plugin-linter",
  "keywords": [
    "eslint",
    "typescript",
    "vite-plugin"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://unimorphic@bitbucket.org/unimorphic/vite-plugin-linter.git"
  },
  "types": "dist/index.d.ts"
}
