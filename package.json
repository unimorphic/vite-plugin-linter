{
  "name": "vite-plugin-linter",
  "description": "Plugin for linting files with Vite",
  "license": "MIT",
  "version": "3.0.0",
  "scripts": {
    "build": "unbuild",
    "prepare": "npm run build"
  },
  "bin": "./dist/lintCommand.mjs",
  "dependencies": {
    "@rollup/pluginutils": "5.1.0",
    "@types/eslint": "8.56.10",
    "chokidar": "3.6.0",
    "eslint": "9.7.0",
    "typescript": "5.5.3"
  },
  "devDependencies": {
    "@types/node": "20.14.10",
    "prettier": "3.3.3",
    "rollup": "4.18.1",
    "unbuild": "2.0.0",
    "vite": "5.3.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://bitbucket.org/unimorphic/vite-plugin-linter",
  "keywords": [
    "eslint",
    "typescript",
    "vite-plugin"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://unimorphic@bitbucket.org/unimorphic/vite-plugin-linter.git"
  },
  "types": "dist/index.d.ts"
}
