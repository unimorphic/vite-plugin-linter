{
  "name": "vite-plugin-linter",
  "description": "Plugin for linting files with Vite",
  "license": "MIT",
  "version": "3.1.1",
  "scripts": {
    "build": "unbuild",
    "prepare": "npm run build"
  },
  "bin": "./dist/lintCommand.mjs",
  "dependencies": {
    "@rollup/pluginutils": "5.3.0",
    "@types/eslint": "9.6.1",
    "chokidar": "4.0.3",
    "eslint": "9.36.0",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "prettier": "3.6.2",
    "rollup": "4.52.0",
    "unbuild": "3.6.1",
    "vite": "7.1.7"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/unimorphic/vite-plugin-linter",
  "keywords": [
    "eslint",
    "typescript",
    "vite-plugin"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/unimorphic/vite-plugin-linter.git"
  },
  "types": "dist/index.d.ts"
}
